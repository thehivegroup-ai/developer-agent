# PostgreSQL Configuration
POSTGRES_HOST=dh02
POSTGRES_PORT=5432
POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DB=a2a_agents

# Neo4j Configuration (Docker)
NEO4J_URI=neo4j://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password
NEO4J_DATABASE=a2a_agents

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# GitHub Configuration (Optional - for higher rate limits)
GITHUB_TOKEN=your_github_token_here

# Repository Configuration
REPOSITORIES_CONFIG_PATH=./config/repositories.json

# Agent Configuration
AGENT_TTL_SECONDS=3600
AGENT_MAX_POOL_SIZE=10

# Application Configuration
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# WebSocket Configuration
WS_PORT=3001

# Rate Limiting
GITHUB_RATE_LIMIT_WARNING_THRESHOLD=50
OPENAI_RATE_LIMIT_WARNING_THRESHOLD=100
