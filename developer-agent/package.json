{
  "name": "@developer-agent/developer-agent",
  "version": "0.1.0",
  "description": "Developer Agent - Central orchestrator for multi-agent system",
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "a2a": "tsx src/a2a-server.ts",
    "query": "tsx src/cli.ts",
    "search": "tsx src/search.ts",
    "relationships": "tsx src/relationships.ts",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:integration": "tsx tests/test-agent-integration.ts",
    "test:workflow": "tsx tests/test-workflow.ts",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@developer-agent/shared": "*",
    "@developer-agent/github-agent": "*",
    "@developer-agent/repository-agents": "*",
    "@langchain/core": "^0.3.0",
    "@langchain/langgraph": "^0.2.0",
    "@langchain/openai": "^0.3.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  }
}
