{
  "name": "developer-agent",
  "version": "0.1.0",
  "description": "A2A Multi-Agent System for analyzing GitHub repositories using LangGraph",
  "private": true,
  "type": "module",
  "workspaces": [
    "shared",
    "api-gateway",
    "developer-agent",
    "github-agent",
    "relationship-agent",
    "repository-agents",
    "frontend"
  ],
  "scripts": {
    "dev": "npm run dev --workspaces --if-present",
    "start:all": "concurrently -n \"api,frontend,dev-agent,gh-agent,rel-agent,repo-agent\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold,bgYellow.bold,bgCyan.bold,bgRed.bold\" \"npm run dev -w api-gateway\" \"npm run dev -w frontend\" \"npm run dev -w developer-agent\" \"npm run dev -w github-agent\" \"npm run dev -w relationship-agent\" \"npm run dev -w repository-agents\"",
    "build": "npm run build --workspaces --if-present",
    "test": "node scripts/test-summary.js",
    "test:watch": "npm run test:watch --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:setup": "npm run db:setup -w api-gateway",
    "db:migrate": "npm run db:migrate -w api-gateway",
    "db:seed": "npm run db:seed -w api-gateway",
    "db:reset": "npm run db:reset -w api-gateway",
    "demo:ai": "tsx scripts/demo-openai.ts"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "chalk": "^5.6.2",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}
